<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h1>
      Cuisineagree #<%= @cuisineagree.id %> details
    </h1>

    <div class="row mb-3">
      <div class="col">
        <a href="/cuisineagrees" class="btn btn-block btn-outline-secondary">
          Go back
        </a>
      </div>

      <div class="col">
        <a href="/delete_cuisineagree/<%= @cuisineagree.id %>" class="btn btn-block btn-outline-secondary">
          Delete cuisineagree
        </a>
      </div>
    </div>

    <dl>
      <dt>
        Ca
      </dt>
      <dd>
        <%= @cuisineagree.ca_id %>
      </dd>

      <dt>
        Cfood
      </dt>
      <dd>
        <%= @cuisineagree.cfood_id %>
      </dd>

      <dt>
        Created at
      </dt>
      <dd>
        <%= time_ago_in_words(@cuisineagree.created_at) %> ago
      </dd>

      <dt>
        Updated at
      </dt>
      <dd>
        <%= time_ago_in_words(@cuisineagree.updated_at) %> ago
      </dd>
    </dl>
  </div>
</div>

<hr>


<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h2>
      Edit cuisineagree
    </h2>

    <form action="/modify_cuisineagree/<%= @cuisineagree.id %>"  method="post" >
      <div class="form-group">
        <label for="ca_id_box">
          Ca
        </label>

        <input type="text" id="ca_id_box" name="query_ca_id" class="form-control" value="<%= @cuisineagree.ca_id %>">
      </div>

      <div class="form-group">
        <label for="cfood_id_box">
          Cfood
        </label>

        <input type="text" id="cfood_id_box" name="query_cfood_id" class="form-control" value="<%= @cuisineagree.cfood_id %>">
      </div>

      <button class="btn btn-block btn-outline-secondary">
        Update cuisineagree
      </button>
    </form>
  </div>
</div>

<hr>
